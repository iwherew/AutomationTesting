      }
    }
  },
  {
    "description":"这是一个带cookies的post请求",
    "request":{
      "uri":"/post/with/cookies",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"wang",
        "age":"17"
      }
    },
    "response":{
      "json":{
        "name":"wwwwww",
        "age":"1111111"
      }
    }
  },
  {
    "description":"带header信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "forms": {
        "name": "wang",
        "age": "17"
      }
    },
    "response":{
      "json":{
        "status":"111",
        "s":"success"
      }
    }
  }
